<?xml version="1.0" encoding="UTF-8"?>
<!-- 机构  -->
<tekinfo>
  <object>
    <Organization ResourceBundle="com.takall.organization.Organization"
        Structure="com.takall.organization.Organization"
        ObjectRm="com.takall.remoting.organization.OrganizationRm">
      <autovalue>
        <organization_id base="identity"/>
        <organization_owner base="userid"/>
      </autovalue>
      
      <nosamevalue>
        <organization_id/>
        <organization_code/>
        <organization_name/>
      </nosamevalue>
      
      <table nameIdentity="organization_name" codeIdentity="null" ownerIdentity="null">
        <organization identity="organization_id"/>
        <organization_icon identity="organization_id"/>
        <organization_icon_blob identity="organization_id"/>
      </table>
      
      <blob blobObject="OrganizationFile"
          table="null"
          urlField="null"
          blobField="null"
          mimeField="null"
          lengthField="null"
          timeField="null"
          mimeType="text/plain"/>

      <icon table="organization_icon"
          blobTable="organization_icon_blob"
          urlField="null"
          blobField="icon_blob"
          mimeField="icon_mime"
          lengthField="icon_length"
          timeField="icon_time"
          supportSuffix="png,jpg,gif"
          maxLength="1"
          minWidth="100"
          maxWidthe="0"
          minHeight="100"
          maxHeight="0"/>

      <command>
        <add>
          <parameter>
            <!-- 0x00 不记录；0x01 只记录所有用户的最近一次操作；0x10 记录当前用户最近一次操作；0x80 每一次都记录 -->
            <record value="0x80"/>

            <!-- 最大标签数 -->
            <max_tags value="5"/>
            <!-- 默认联系人阅读属性 -->
            <contact_property value="64"/>
          </parameter>
          <item>
            <organization_code/>
            <organization_name/>
            <organization_person/>
            <organization_leader/>
            <organization_type forbidEdit="true"/>
            <organization_catalog/>
            <organization_tags/>
            <organization_url/>
            
            <organization_remark/>
            
            <organization_status/>
 
            <organization_country rightrole="auditor"/>
            <organization_state rightrole="auditor"/>
            <organization_city rightrole="auditor"/>
            <organization_street rightrole="auditor"/>
          </item>
        </add>

        <edit>
          <parameter>
            <!-- 0x00 不记录；0x01 只记录所有用户的最近一次操作；0x10 记录当前用户最近一次操作；0x80 每一次都记录 -->
            <record value="0x80"/>

            <!-- 最大标签数 -->
            <max_tags value="5"/>
          </parameter>
          <item>
            <organization_code/>
            <organization_name/>
            <organization_person/>
            <organization_leader/>
            <organization_type forbidEdit="true"/>
            <organization_catalog/>
            <organization_tags/>
            <organization_url/>
            
            <organization_remark/>
            
            <organization_status/>
 
            <organization_country rightrole="auditor"/>
            <organization_state rightrole="auditor"/>
            <organization_city rightrole="auditor"/>
            <organization_street rightrole="auditor"/>
          </item>
        </edit>
        
        <!-- 审核 -->
        <audit>
          <parameter>
            <!-- 是否新建操作 -->
            <add value="false"/>
            <!-- 0x00 不记录；0x01 只记录所有用户的最近一次操作；0x10 记录当前用户最近一次操作；0x80 每一次都记录 -->
            <record value="0x80"/>
            <!-- 命令类型（以“,”分割多种类型）：
                 “edit”：编辑
                 “read”：读取
                 “remove”：删除
                 “admin”：系统管理 -->
            <command-type value="edit"/>
          </parameter>
          <item>
            <organization_status/>
          </item>
        </audit>

        <!-- 绑定用户 -->
        <bind>
          <parameter>
            <!-- 是否新建操作 -->
            <add value="false"/>
            <!-- 0x00 不记录；0x01 只记录所有用户的最近一次操作；0x10 记录当前用户最近一次操作；0x80 每一次都记录 -->
            <record value="0x80"/>
            <!-- 命令类型（以“,”分割多种类型）：
                 “edit”：编辑
                 “read”：读取
                 “remove”：删除
                 “admin”：系统管理 -->
            <command-type value="edit"/>
          </parameter>
          <item>
            <organization_owner/>
          </item>
        </bind>

        <read>
          <parameter>
            <!-- 0x00 不记录；0x01 只记录所有用户的最近一次操作；0x10 记录当前用户最近一次操作；0x80 每一次都记录 -->
            <record value="0x10"/>
          </parameter>
          <item>
            <organization_code/>
            <organization_name/>
            <organization_person/>
            <organization_leader/>
            <organization_type forbidEdit="true"/>
            <organization_catalog/>
            <organization_tags/>
            <organization_url/>
            
            <organization_remark/>
            
            <organization_status/>
 
            <organization_country rightrole="auditor"/>
            <organization_state rightrole="auditor"/>
            <organization_city rightrole="auditor"/>
            <organization_street rightrole="auditor"/>
          </item>
        </read>
        
        <list>
          <parameter>
            <!-- 0x00 不记录；0x01 只记录所有用户的最近一次操作；0x10 记录当前用户最近一次操作；0x80 每一次都记录 -->
            <record value="0x01"/>
            <list_counts/>
          </parameter>
          <item>
            <organization_code/>
            <organization_name/>
            <organization_person/>
            <organization_leader/>
            <organization_type forbidEdit="true"/>
            <organization_catalog/>
            <organization_tags/>
            <organization_url/>
            
            <organization_status/>
 
            <organization_country rightrole="auditor"/>
            <organization_state rightrole="auditor"/>
            <organization_city rightrole="auditor"/>
            <organization_street rightrole="auditor"/>
          </item>
        </list>
        
        <remove>
          <parameter>
            <remove_sub value="true"/>
            <!-- 0x00 不记录；0x01 只记录所有用户的最近一次操作；0x10 记录当前用户最近一次操作；0x80 每一次都记录 -->
            <record value="0x80"/>
          </parameter>
        </remove>
        
        <search>
          <parameter>
            <list_counts/>
          </parameter>
          <item>
            <organization_code/>
            <organization_name/>
            <organization_person/>
            <organization_type/>
            <organization_catalog/>
            <organization_tags/>
            <organization_url/>
            <organization_count/>
            <organization_support/>

            <organization_country/>
            <organization_state/>
            <organization_city/>
            <organization_street/>
          </item>
        </search>
        
        <textSearch>
          <parameter>
            <list_counts/>
            <!-- 0x00 不记录；0x01 只记录所有用户的最近一次操作；0x10 记录当前用户最近一次操作；0x80 每一次都记录 -->
            <record value="0x0"/>
          </parameter>
          <item>
            <organization_code/>
            <organization_name/>
            <!-- <organization_catalog/>
            <organization_tags/> -->
            <organization_status/>
            <organization_owner/>
            <organization_remark/>
          </item>
        </textSearch>
        
        <upload>
          <parameter>
            <!-- 支持的文件后缀名（以“,”分割）-->
            <support-suffix value="png,jpg,gif,html"/>
            <!-- 允许的文件最大长度（MB） -->
            <max-length value="1"/>

            <!-- 是否是图片文件 -->
            <image-file value="true"/>
            <!-- 图片最小宽度 -->
            <min-imageWidth value="200"/>
            <!-- 图片最大宽度 -->
            <max-imageWidth value="0"/>
            <!-- 图片最小高度 -->
            <min-imageHeight value="200"/>
            <!-- 图片最大高度 -->
            <max-imageHeight value="0"/>
          </parameter>
        </upload>
        
        <download/>
      </command>
      
      <property>
        <!-- 标识 -->
        <organization_id variable="long" notnull="true" forbidEdit="true"/>
        <!-- 编码 -->
        <organization_code variable="string" maxlength="128" notnull="true"/>
        <!-- 名称 -->
        <organization_name variable="string" maxlength="128" notnull="true"/>
        
        <!-- 法人 -->
        <organization_person variable="string" maxlength="32"/>
        <!-- 负责人 -->
        <organization_leader variable="string" maxlength="128"/>
        
        <!-- 类型 -->
        <organization_type variable="int" parse="enum" format="single" default="0" forbidEdit="true" notnull="true">
          <unknown value="0"/>  <!--   未知 -->
          <government value="0x01"/>    <!--  政府 -->
          <association value="0x20"/>   <!-- 协会 -->
          <nonprofit value="0x40"/>    <!--  非营利性组织 -->
          <profit value="0x80"/>   <!--  商业机构 -->
          <expert value="0x0100"/>      <!--  授权合伙人 -->
        </organization_type>
        
        <!-- 目录分类 -->
        <organization_catalog variable="string" maxlength="128" parse="catalog"/>
        <!-- 标签分类 -->
        <organization_tags variable="string" maxlength="256" parse="tag"/>
        <!-- 网址 -->
        <organization_url variable="string" maxlength="255"/>
        <!-- 说明 -->
        <organization_remark variable="string" format="textarea:4"/>
        
        <!-- 状态 -->
        <organization_status variable="int" parse="enum" format="single" default="5" notnull="true">
          <stop value="-1"/>      <!-- 停用 -->
          <apply value="0"/>      <!-- 待审核 -->
          <valid value="10"/>     <!-- 有效 -->
        </organization_status>
        
        <!-- 所有者 -->
        <organization_owner variable="long" parse="object:User" default="0" notnull="true"/>
        
        <!-- 国家（标准的2字节代码） -->
        <organization_country variable="string" parse="country" format="option"/>
        <!-- 省（州）二级行政单位 -->
        <organization_state variable="string"/>
        <!-- 县（市、区）三级行政单位 -->
        <organization_city variable="string"/>
        <!-- 街道 -->
        <organization_street variable="string" maxlength="255"/>

        <createTime/>
        <createIp/>
        <modifyTime/>
        <modifyIp/>
        <accessTime/>
        <accessIp/>
        <accessCount/>
      </property>
      <attachment>
        <!-- 附件对象：
            name：附件名称（自定义）
            object：附件对象名
            column：附件对象记录当前对象值的字段名
            remove：附件删除类型（0 - 删除附件信息；1 - 不删除附件信息；2 - 存在附件信息，禁止删除） -->
        <hd_equipment name="HDEquipment" object="HDEquipment" column="hd_equipment_organization" remove="0"/>
      </attachment>
    </Organization>
  </object>
</tekinfo>
