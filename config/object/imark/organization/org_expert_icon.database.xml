<?xml version="1.0" encoding="UTF-8"?>
<!-- 专家照片头像  -->
<tekinfo>
  <database>
    <table>
      <org_expert_icon>
        <item>
          <!-- 标识 -->
          <org_expert_id value="BIGINT NOT NULL PRIMARY KEY"/>

          <!-- 图标长度 -->
          <icon_length value="BIGINT DEFAULT '0' NOT NULL"/>
          <!-- 图标MIME格式 -->
          <icon_mime value="VARCHAR(512) NOT NULL"/>
          <!-- 图标修改时间 -->
          <icon_time value="BIGINT DEFAULT '0' NOT NULL"/>
        </item>
      </org_expert_icon>
    </table>
  </database>
</tekinfo>