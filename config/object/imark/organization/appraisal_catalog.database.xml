<?xml version="1.0" encoding="UTF-8"?>
<!-- 检验鉴定服务目录附加信息 -->
<tekinfo>
  <database>
    <table>
      <appraisal_catalog>
        <item>
          <!-- 标识 -->
          <appraisal_catalog_id value="BIGINT NOT NULL PRIMARY KEY"/>
          <!-- 编码 -->
          <appraisal_catalog_code value="VARCHAR(10) NOT NULL"/>
          <!-- 名称 -->
          <appraisal_catalog_name value="VARCHAR(128) NOT NULL"/>
          <!-- 单位 -->
          <appraisal_catalog_unit value="VARCHAR(32) NOT NULL"/>
          <!-- 最低基准价 -->
          <appraisal_catalog_min value="MONEY DEFAULT '0' NOT NULL"/>
          <!-- 最高基准价 -->
          <appraisal_catalog_max value="MONEY DEFAULT '0' NOT NULL"/>
          <!-- 说明 -->
          <appraisal_catalog_remark value="MEMO"/>
          <!-- 所属目录 -->
          <appraisal_catalog_catalog value="VARCHAR(128) NOT NULL"/>
          
          <createTime value="VARCHAR(255) NOT NULL"/>
          <createIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <modifyTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <modifyIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessCount value="INT DEFAULT '0' NOT NULL"/>
        </item>
      </appraisal_catalog>
    </table>
  </database>
</tekinfo>