<?xml version="1.0" encoding="UTF-8"?>
<!-- 机构头像支持  -->
<tekinfo>
  <database>
    <table>
      <organization_icon>
        <item>
          <!-- 标识 -->
          <organization_id value="BIGINT NOT NULL PRIMARY KEY"/>

          <!-- 图标长度 -->
          <icon_length value="BIGINT DEFAULT '0' NOT NULL"/>
          <!-- 图标MIME格式 -->
          <icon_mime value="VARCHAR(512) NOT NULL"/>
          <!-- 图标修改时间 -->
          <icon_time value="BIGINT DEFAULT '0' NOT NULL"/>
        </item>
      </organization_icon>
    </table>
  </database>
</tekinfo>