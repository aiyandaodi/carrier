<?xml version="1.0" encoding="UTF-8"?>
<!-- 专家 -->
<tekinfo>
  <object>
    <Expert ResourceBundle="com.takall.object.expert.Expert"
        Structure="com.takall.expert.Expert"
        ObjectRm="com.takall.remoting.expert.ExpertRm">
      <autovalue>
        <expert_id base="identity"/>
        <expert_code base="" step="1" digit="8" tree="default"/>
        <expert_owner base="userid"/>
      </autovalue>
      <nosamevalue>
        <expert_id/>
        <expert_code/>
      </nosamevalue>
      
      <table nameIdentity="expert_name" codeIdentity="null" ownerIdentity="null">
        <expert identity="expert_id"/>
        <expert_icon identity="expert_id"/>
        <expert_icon_blob identity="expert_id"/>
      </table>
      
      <blob blobObject="ExpertFile"
          table="null"
          urlField="null"
          blobField="null"
          mimeField="null"
          lengthField="null"
          timeField="null"
          mimeType="text/plain"/>
      
      <icon table="expert_icon"
          blobTable="expert_icon_blob"
          urlField="null"
          blobField="icon_blob"
          mimeField="icon_mime"
          lengthField="icon_length"
          timeField="icon_time"
          supportSuffix="png,jpg,jpeg,gif"
          maxLength="1"
          minWidth="100"
          maxWidth="0"
          minHeight="100"
          maxHeight="0"/>
      
      <command>
        <add>
          <parameter>
            <!-- 0x00 不记录；0x01 只记录所有用户的最近一次操作；0x10 记录当前用户最近一次操作；0x80 每一次都记录 -->
            <record value="0x80"/>
            
            <!-- 默认联系人阅读属性 -->
            <contact_property value="1"/>
            
            <!-- 小组发言属性 -->
            <group_speak value="0x08"/>
            <!-- 小组阅读属性 -->
            <group_listen value="0x40"/>
            <!-- 小组加入属性 -->
            <group_join value="0x00"/>
            <!-- 小组组员对组员权限 -->
            <group_member_memberright value="0x00"/>
            <!-- 小组组员对主题权限 -->
            <group_member_subjectright value="0x00"/>
          </parameter>
          <item>
            <expert_name/>
            <expert_gender/>
            <expert_catalog/>
            <expert_tags/>
            <expert_employ/>
            <expert_title/>
            <expert_job/>
            <expert_work/>
            <expert_licence/>
            <expert_support/>
            <expert_remark/>
          </item>
        </add>
        
        <edit>
          <parameter>
            <!-- 0x00 不记录；0x01 只记录所有用户的最近一次操作；0x10 记录当前用户最近一次操作；0x80 每一次都记录 -->
            <record value="0x80"/>
          </parameter>
          <item>
            <expert_code rightrole="auditor"/>
            <expert_name/>
            <expert_gender/>
            <expert_catalog/>
            <expert_tags/>
            <expert_employ/>
            <expert_title/>
            <expert_job/>
            <expert_work/>
            <expert_licence/>
            <expert_status/>
            <expert_support/>
            <expert_remark/>
            
            <expert_country rightrole="auditor"/>
            <expert_state rightrole="auditor"/>
            <expert_city rightrole="auditor"/>
            <expert_street rightrole="auditor"/>
          </item>
        </edit>
        
        <!-- 变更 -->
        <change>
          <parameter>
            <!-- 是否新建操作 -->
            <add value="false"/>
            <!-- 0x00 不记录；0x01 只记录所有用户的最近一次操作；0x10 记录当前用户最近一次操作；0x80 每一次都记录 -->
            <record value="0x80"/>
            <!-- 命令类型（以“,”分割多种类型）：
                 “edit”：编辑
                 “read”：读取
                 “remove”：删除
                 “admin”：系统管理 -->
            <command-type value="edit"/>
          </parameter>
          <item>
            <expert_workingtime/>
          </item>
        </change>
        
        <!-- 加入机构 -->
        <join>
          <parameter>
            <!-- 是否新建操作 -->
            <add value="false"/>
            <!-- 0x00 不记录；0x01 只记录所有用户的最近一次操作；0x10 记录当前用户最近一次操作；0x80 每一次都记录 -->
            <record value="0x80"/>
            <!-- 命令类型（以“,”分割多种类型）：
                 “edit”：编辑
                 “read”：读取
                 “remove”：删除
                 “admin”：系统管理 -->
            <command-type value="edit"/>
          </parameter>
          <item>
            <expert_organization/>
          </item>
        </join>
        
        <!-- 绑定用户 -->
        <bind>
          <parameter>
            <!-- 是否新建操作 -->
            <add value="false"/>
            <!-- 0x00 不记录；0x01 只记录所有用户的最近一次操作；0x10 记录当前用户最近一次操作；0x80 每一次都记录 -->
            <record value="0x80"/>
            <!-- 命令类型（以“,”分割多种类型）：
                 “edit”：编辑
                 “read”：读取
                 “remove”：删除
                 “admin”：系统管理 -->
            <command-type value="edit"/>
          </parameter>
          <item>
            <expert_user/>
          </item>
        </bind>
        
        <!-- 审核 -->
        <audit>
          <parameter>
            <!-- 是否新建操作 -->
            <add value="false"/>
            <!-- 0x00 不记录；0x01 只记录所有用户的最近一次操作；0x10 记录当前用户最近一次操作；0x80 每一次都记录 -->
            <record value="0x80"/>
            <!-- 命令类型（以“,”分割多种类型）：
                 “edit”：编辑
                 “read”：读取
                 “remove”：删除
                 “admin”：系统管理 -->
            <command-type value="edit"/>
          </parameter>
          <item>
            <expert_systemPriority/>
            <expert_status/>
          </item>
        </audit>
        
        <read>
          <parameter>
            <!-- 0x00 不记录；0x01 只记录所有用户的最近一次操作；0x10 记录当前用户最近一次操作；0x80 每一次都记录 -->
            <record value="0x10"/>
          </parameter>
          <item>
            <expert_code rightrole="auditor"/>
            <expert_name/>
            <expert_gender/>
            <expert_catalog/>
            <expert_tags/>
            <expert_organization/>
            <expert_employ/>
            <expert_title/>
            <expert_job/>
            <expert_work/>
            <expert_licence/>
            <expert_remark/>
            <expert_workingtime/>
            <expert_support/>
            <expert_owner rightrole="auditor;customerService"/>
            <expert_user/>
            <expert_evaluate/>
            <expert_systemPriority rightrole="auditor;customerService"/>
            <expert_status/>
            <expert_country/>
            <expert_state/>
            <expert_city/>
            <expert_street/>
            <createTime rightrole="auditor"/>
          </item>
        </read>
        
        <list>
          <parameter>
            <!-- 0x00 不记录；0x01 只记录所有用户的最近一次操作；0x10 记录当前用户最近一次操作；0x80 每一次都记录 -->
            <record value="0x01"/>
            <list_counts/>
          </parameter>
          <item>
            <expert_name/>
            <expert_gender/>
            <expert_catalog/>
            <expert_tags/>
            <expert_organization/>
            <expert_employ/>
            <expert_title/>
            <expert_job/>
            <expert_work/>
            <expert_licence/>
            <expert_support/>
            <expert_remark/>
            <expert_owner rightrole="auditor;customerService"/>
            <expert_user/>
            <expert_evaluate/>
            <expert_systemPriority rightrole="auditor;customerService"/>
            <expert_status/>
            <expert_country/>
            <expert_state/>
            <expert_city/>
            <expert_street/>
          </item>
        </list>
        
        <remove>
          <parameter>
            <remove_sub value="true"/>
            <!-- 0x00 不记录；0x01 只记录所有用户的最近一次操作；0x10 记录当前用户最近一次操作；0x80 每一次都记录 -->
            <record value="0x80"/>
          </parameter>
        </remove>
        
        <search>
          <parameter>
            <list_counts/>
          </parameter>
          <item>
            <expert_code/>
            <expert_name/>
            <expert_gender/>
            <expert_organization/>
            <expert_employ/>
            <expert_catalog/>
            <expert_tags/>
            <expert_licence/>
            
            <expert_country/>
            <expert_state/>
            <expert_city/>
            <expert_street/>
          </item>
        </search>
        
        <textSearch>
          <parameter>
            <list_counts/>
          </parameter>
          <item>
            <expert_name/>
            <expert_gender/>
            <expert_organization/>
            <expert_employ/>
            <expert_catalog/>
            <expert_tags/>
            <expert_licence/>
          </item>
        </textSearch>
        
        <upload>
          <parameter>
            <!-- 支持的文件后缀名（以“,”分割）-->
            <support-suffix value="png,jpg,gif,html"/>
            <!-- 允许的文件最大长度（MB） -->
            <max-length value="1"/>
            
            <!-- 是否是图片文件 -->
            <image-file value="true"/>
            <!-- 图片最小宽度 -->
            <min-imageWidth value="200"/>
            <!-- 图片最大宽度 -->
            <max-imageWidth value="0"/>
            <!-- 图片最小高度 -->
            <min-imageHeight value="200"/>
            <!-- 图片最大高度 -->
            <max-imageHeight value="0"/>
          </parameter>
        </upload>
        
        <download/>
      </command>
      
      <property>
        <!-- 标识 -->
        <expert_id variable="long" notnull="true" forbidEdit="true"/>
        <!-- 编码 -->
        <expert_code variable="string" maxlength="128" notnull="true" forbitEdit="true"/>
        <!-- 名称 -->
        <expert_name variable="string" maxlength="32" notnull="true"/>
        
        <!-- 性别 -->
        <expert_gender variable="int" parse="enum" format="single" default="0x01" notnull="true">
          <!-- <none value="0x00"/>     未知 -->
          <man value="0x01"/>      <!-- 男 -->
          <woman value="0x02"/>    <!-- 女 -->
        </expert_gender>
        
        <!-- 目录分类 -->
        <expert_catalog variable="string" maxlength="255" parse="catalog:Expert"/>
        <!-- 标签分类 -->
        <expert_tags variable="string" parse="tag" maxlength="255"/>
        
        <!-- 所属机构 -->
        <expert_organization variable="long" parse="object:Organization" notnull="true"/>
        <!-- 就职单位 -->
        <expert_employ variable="string" maxlength="255"/>
        <!-- 职称 -->
        <expert_title variable="string" maxlength="255"/>
        <!-- 职务 -->
        <expert_job variable="string" maxlength="255"/>
        
        <!-- 执业证号 -->
        <expert_licence variable="string" maxlength="128"/>
        <!-- 从业年限 -->
        <expert_work variable="int" parse="int" default="0"/>
        
        <!-- 专家说明 -->
        <expert_remark variable="string" format="textarea:4"/>
        
        <!-- 创建者 -->
        <expert_owner variable="long" parse="object:User" default="0" notnull="true"/>
        <!-- 所属用户 -->
        <expert_user variable="long" parse="object:User" default="0" notnull="true"/>
        <!-- 系统优先级 -->
        <expert_systemPriority variable="int" parse="int" default="1" notnull="true" />
        <!-- 状态 -->
        <expert_status variable="int" parse="enum" format="single" default="5" notnull="true">
          <stop value="-1"/>      <!-- 停用 -->
          <modify value="0"/>     <!-- 正在修改 -->
          <apply value="5"/>      <!-- 待审核 -->
          <normal value="10"/>    <!-- 正常 -->
        </expert_status>
        <!-- 附加信息 -->
        <expert_support variable="int" parse="enum" format="multiple" default="0x00" notnull="true">
          <none value="0x00"/>     <!-- 无 -->
          <aid value="0x01"/>      <!-- 提供援助 -->
          <court value="0x04"/>    <!-- 出庭质证 -->
          <hearing value="0x10"/>   <!-- 参加听证会 -->
          <live value="0x40"/>     <!-- 现场见证 -->
        </expert_support>
        <!-- 工作时间（默认值：周一~周五） -->
        <expert_workingtime variable="long" default="36" notnull="true"/>
        <!-- 评分 -->
        <expert_evaluate variable="int" parse="float" format="0.0"/>
        
        <!-- 国家（标准的2字节代码） -->
        <expert_country variable="string" parse="country" format="option"/>
        <!-- 省（州）二级行政单位 -->
        <expert_state variable="string"/>
        <!-- 县（市、区）三级行政单位 -->
        <expert_city variable="string"/>
        <!-- 街道 -->
        <expert_street variable="string" maxlength="255"/>
        
        <!-- 虚拟域：审核错误原因 -->
        <expert_reason variable="string" format="textarea:4"/>
        
        <createTime/>
        <createIp/>
        <modifyTime/>
        <modifyIp/>
        <accessTime/>
        <accessIp/>
        <accessCount/>
      </property>
    </Expert>
  </object>
</tekinfo>
