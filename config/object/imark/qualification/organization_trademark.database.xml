<?xml version="1.0" encoding="UTF-8"?>
<!-- 品牌对象 -->
<tekinfo>
  <database>
    <table>
      <!-- 数据库表名 -->
      <organization_trademark>
        <item>
          <!-- 标识 -->
          <trademark_id value="BIGINT NOT NULL PRIMARY KEY"/>
          <!-- 编码 -->
          <trademark_code value="VARCHAR(32) NOT NULL"/>
          <!-- 品牌：名称 -->
          <trademark_name value="VARCHAR(255) NOT NULL"/>
          <!-- 品牌：所有者名 -->
          <trademark_authorizer value="VARCHAR(255)"/>
          <!-- 品牌：目录分类 -->
          <trademark_catalog value="VARCHAR(128)"/>
          <!-- 品牌：标签分类 -->
          <trademark_tag value="VARCHAR(200)"/>
          <!-- 品牌: 国别 -->
          <trademark_country value="CHAR(2) DEFAULT 'CN'"/>
          <!-- 品牌: 注册日期 -->
          <trademark_start value="BIGINT DEFAULT '19700101' NOT NULL"/>
          <!-- 品牌: 状态 -->
          <trademark_status value="SMALLINT DEFAULT '0' NOT NULL"/>
          <!-- 品牌: 所属机构  -->
          <trademark_organization value="VARCHAR(32) NOT NULL"/>
          
          <createTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <createIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <modifyTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <modifyIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessCount value="INT DEFAULT '0' NOT NULL"/>
        </item>
      </organization_trademark>
    </table>
  </database>
</tekinfo>