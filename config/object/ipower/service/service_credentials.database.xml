<?xml version="1.0" encoding="UTF-8"?>
<!-- 服务凭证 -->
<tekinfo>
  <database>
    <table>
      <service_credentials>
        <item>
          <!-- 标识 -->
          <service_credentials_id value="BIGINT NOT NULL PRIMARY KEY"/>
          <!-- 编码 -->
          <service_credentials_code value="VARCHAR(64) NOT NULL"/>
          <!-- 服务凭证名 -->
          <service_credentials_name value="VARCHAR(250) NOT NULL"/>
          <!-- 关联主机标识 -->
          <service_credentials_site value="BIGINT DEFAULT '0'"/>
          <!-- 关联主机服务标识 -->
          <service_credentials_service value="BIGINT DEFAULT '0'"/>
          <!-- 凭证用户名 -->
          <service_credentials_username value="VARCHAR(250) NOT NULL"/>
          <!-- 凭证保密字 -->
          <service_credentials_passwd value="VARCHAR(250) NOT NULL"/>
          <!-- 凭证类型 --> 
          <service_credentials_type value="BIGINT DEFAULT '0' NOT NULL"/>
          <!-- 凭证状态 --> 
          <service_credentials_status value="SMALLINT DEFAULT '0' NOT NULL"/>
          <!-- 凭证所属用户 -->
          <service_credentials_user value="BIGINT DEFAULT '0' NOT NULL"/>
          <!-- 凭证所有者 -->
          <service_credentials_owner value="BIGINT DEFAULT '0' NOT NULL"/>
          
          
          <createTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <createIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <modifyTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <modifyIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessCount value="INT DEFAULT '0' NOT NULL"/>
        </item>
      </service_credentials>
    </table>
  </database>
</tekinfo>