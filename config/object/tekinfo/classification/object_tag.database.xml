<?xml version="1.0" encoding="UTF-8"?>
<!-- 对象标签分类 -->
<tekinfo>
  <database>
    <table>
      <object_tag>
        <item>
          <!-- 标识 -->
          <tag_id value="BIGINT NOT NULL PRIMARY KEY"/>
          <!-- 编码 -->
          <tag_code value="VARCHAR(128) NOT NULL"/>
          <!-- 名称 -->
          <tag_name value="VARCHAR(255) NOT NULL"/>

          <!-- 所属对象类型名 -->
          <tag_object value="VARCHAR(255) NOT NULL"/>
          
          <!-- 所有者 -->
          <tag_owner value="BIGINT DEFAULT '0' NOT NULL"/>
          
          <!-- 语言（例如：zh_CN en_US 等） -->
          <tag_language value="CHAR(5) NOT NULL"/>
          
          <createTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <createIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <modifyTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <modifyIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessCount value="INT DEFAULT '0' NOT NULL"/>
        </item>
      </object_tag>
    </table>
  </database>
</tekinfo>