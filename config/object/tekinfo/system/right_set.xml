<?xml version="1.0" encoding="UTF-8"?>
<!-- 权限组 -->

<tekinfo>
  <database>
    <table>
      <right_set>
        <item>
          <!-- 标识 -->
          <rightset_id value="BIGINT NOT NULL PRIMARY KEY"/>
          <!-- 编码 -->
          <rightset_code value="VARCHAR(6) NOT NULL"/>
          <!-- 权限组名 -->
          <rightset_name value="VARCHAR(20) NOT NULL"/>
          <!-- 权限组所属对象名 -->
          <rightset_object value="VARCHAR(32) NOT NULL"/>
          <!-- 权限组所属对象 -->
          <rightset_objectId value="BIGINT DEFAULT '0' NOT NULL"/>
          <!-- 目标对象名 -->
          <rightset_target value="VARCHAR(32) NOT NULL"/>
          <!-- 目标命令 -->
          <rightset_command value="VARCHAR(255) NOT NULL"/>
          <!-- 有效期开始时间 -->
          <rightset_start value="BIGINT DEFAULT '0' NOT NULL"/>
          <!-- 有效期结束时间 -->
          <rightset_end value="BIGINT DEFAULT '0' NOT NULL"/>
          
          <createTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <createIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <modifyTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <modifyIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessCount value="INT DEFAULT '0' NOT NULL"/>
        </item>
      </right_set>
    </table>
  </database>
</tekinfo>