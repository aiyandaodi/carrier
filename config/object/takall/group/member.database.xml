<?xml version="1.0" encoding="UTF-8"?>
<!-- 组员  -->
<tekinfo>
  <database>
    <table>
      <member>
        <item>
          <!-- 标识 -->
          <member_id value="BIGINT NOT NULL PRIMARY KEY"/>
          <!-- 编码 -->
          <member_code value="VARCHAR(128) NOT NULL"/>
          <!-- 名称 -->
          <member_name value="VARCHAR(32) NOT NULL"/>
          <!-- 状态 -->
          <member_status value="SMALLINT DEFAULT '0' NOT NULL"/>
          <!-- 信用度 -->
          <member_credit value="INT DEFAULT '0' NOT NULL"/>
          <!-- 备注 -->
          <member_remark value="MEMO"/>
          <!-- 权限 -->
          <member_right value="INT DEFAULT '0' NOT NULL"/>
          <!-- 主题接收方式 -->
          <member_inform value="SMALLINT DEFAULT '0' NOT NULL"/>
          <!-- 电子邮箱 -->
          <member_email value="VARCHAR(32)"/>
          <!-- 手机号码 -->
          <member_mobile value="VARCHAR(20)"/>
          <!-- 即时通号码 -->
          <member_instant value="VARCHAR(64)"/>
          <!-- 所属群组 -->
          <member_group value="BIGINT DEFAULT '0' NOT NULL"/>
          <!-- 所属系统用户 -->
          <member_user value="BIGINT DEFAULT '0' NOT NULL"/>
          <!-- 创建者 -->
          <member_creator value="BIGINT DEFAULT '0' NOT NULL"/>
          <!-- 成员最近访问组时间 -->
          <member_latest value="BIGINT DEFAULT '0' NOT NULL"/>
          
          <createTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <createIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <modifyTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <modifyIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessCount value="INT DEFAULT '0' NOT NULL"/>
        </item>
      </member>
    </table>
  </database>
</tekinfo>