<?xml version="1.0" encoding="UTF-8"?>
<!-- 城市 -->
<tekinfo>
  <database>
    <table>
      <city>
        <item>
          <!-- 标识 -->
          <city_id value="BIGINT NOT NULL PRIMARY KEY"/>
          <!-- 编码 -->
          <city_code value="VARCHAR(14) NOT NULL"/>
          <!-- 简称 -->
          <city_name value="VARCHAR(64) NOT NULL"/>
          <!-- 全称 -->
          <city_fullname value="VARCHAR(64) NOT NULL"/>
          <!-- 所属省/州 -->
          <city_state value="VARCHAR(10) NOT NULL"/>
          <!-- 区域 -->
          <city_locale value="VARCHAR(10) NOT NULL"/>
          <!-- 是否有效 -->
          <city_valid value="SMALLINT DEFAULT '0' NOT NULL"/>
          
          <createTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <createIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <modifyTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <modifyIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessCount value="INT DEFAULT '0' NOT NULL"/>
        </item>
      </city>
    </table>
  </database>
</tekinfo>