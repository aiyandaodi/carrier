<?xml version="1.0" encoding="UTF-8"?>
<!-- 联系信息电子邮箱 -->
<tekinfo>
  <database>
    <table>
      <contact_email>
        <item>
          <!-- 标识 -->
          <email_id value="BIGINT NOT NULL PRIMARY KEY"/>
          <!-- 编码 -->
          <email_code value="VARCHAR(64) NOT NULL"/>
          <!-- 名字 -->
          <email_name value="VARCHAR(128) NOT NULL"/>
          <!-- 电子邮箱地址 -->
          <email_address value="VARCHAR(255) NOT NULL"/>
          <!-- 所属联系信息 -->
          <email_contact value="BIGINT DEFAULT '0' NOT NULL"/>
	        
          <createTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <createIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <modifyTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <modifyIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessCount value="INT DEFAULT '0' NOT NULL"/>
        </item>
      </contact_email>
    </table>
  </database>
</tekinfo>