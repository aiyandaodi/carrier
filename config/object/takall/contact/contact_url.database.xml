<?xml version="1.0" encoding="UTF-8"?>
<!-- 联系信息URL -->
<tekinfo>
  <database>
    <table>
      <contact_url>
        <item>
          <!-- 标识 -->
          <contact_url_id value="BIGINT NOT NULL PRIMARY KEY"/>
          <!-- 编码 -->
          <contact_url_code value="VARCHAR(64) NOT NULL"/>
          <!-- 名字 -->
          <contact_url_name value="VARCHAR(128) NOT NULL"/>
          <!-- URL地址 -->
          <contact_url_address value="VARCHAR(255) NOT NULL"/>
          <!-- 所属联系信息 -->
          <contact_url_contact value="BIGINT DEFAULT '0' NOT NULL"/>
	        
          <createTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <createIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <modifyTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <modifyIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessCount value="INT DEFAULT '0' NOT NULL"/>
        </item>
      </contact_url>
    </table>
  </database>
</tekinfo>