<?xml version="1.0" encoding="UTF-8"?>
<!-- 即时通 -->
<tekinfo>
  <database>
    <table>
      <contact_instance>
        <item>
          <!-- 标识 -->
          <instance_id value="BIGINT NOT NULL PRIMARY KEY"/>
          <!-- 编码 -->
          <instance_code value="VARCHAR(64) NOT NULL"/>
          <!-- 名字 -->
          <instance_name value="VARCHAR(32) NOT NULL"/>
          <!-- 协议类型 -->
          <instance_type value="VARCHAR(32) DEFAULT 'MSN' NOT NULL"/>
          <!-- 即时通地址 -->
          <instance_address value="VARCHAR(255) NOT NULL"/>
          <!-- 所属联系人 -->
          <instance_contact value="BIGINT DEFAULT '0' NOT NULL"/>
	        
          <createTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <createIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <modifyTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <modifyIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessTime value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessIp value="BIGINT DEFAULT '0' NOT NULL"/>
          <accessCount value="INT DEFAULT '0' NOT NULL"/>
        </item>
      </contact_instance>
    </table>
  </database>
</tekinfo>