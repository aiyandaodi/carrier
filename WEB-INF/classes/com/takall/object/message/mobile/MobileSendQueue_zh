name=短信发送队列

#property
property.queue_id=标识
property.queue_code=编码
property.queue_name=名称
property.queue_phone=接收手机号码
property.queue_receiver=接收者
property.queue_sender=发送者
property.queue_object=发送对象名
property.queue_objectid=发送对象
property.queue_time=成功发送时间
property.queue_ontime=定时发送时间
property.queue_times=发送次数
property.queue_message=短信内容
property.queue_length=短信长度
property.queue_status=发送状态
property.queue_status.nosend=未发送
property.queue_status.sent=已发送
property.queue_status.received=已接收
property.queue_status.failure=发送失败
property.queue_sendcode=发送码
property.queue_account=手机账号

property.queue_regular=定时发送
property.queue_virture_phone=接收手机
property.queue_virture_receiver=接收者
property.queue_virture_time=发送时间
property.queue_virture_status=发送状态
property.queue_virture_status.nosend=未发送
property.queue_virture_status.sent=已发送
property.queue_virture_status.received=已接收
property.queue_virture_status.failure=发送失败
property.queue_virture_sendcode=发送码

#message
message.count.remain=剩余短信数
message.sendmessage=发短信

message.error.exceedTime=已经超过发送时间!
message.error.noPhone=没有输入接收手机号码!
message.error.invalidPhone=电话号码无效
message.error.countNotEqual=接收人与手机号码数量不一致!
message.error.noReceivers=没有选择或无法找到接收者!
message.error.invalidSendObject=无效的发送对象
