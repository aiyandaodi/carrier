name=电子邮件账号

#property
property.account_id=标识
property.account_code=编码
property.account_name=名称
property.account_email=电子邮件地址
property.account_recvhost=接收服务器
property.account_recvport=接收服务器端口
property.account_recvuser=接收邮件用户名
property.account_recvpass=接收邮件保密字
property.account_recvtype=接收协议
property.account_recvtype.pop3=POP3
property.account_recvtype.pop3ssl=POP3(SSL)
property.account_recvtype.imap=IMAP
property.account_recvtype.imapssl=IMAP(SSL)

property.account_sendhost=发送服务器
property.account_sendport=发送服务器端口
property.account_senduser=发送邮件用户名
property.account_sendpass=发送邮件保密字
property.account_sendtype=发送协议
property.account_sendtype.smtp=SMTP
property.account_sendtype.smtpssl=SMTP(SSL)
property.account_sendtype.smtptls=SMTP(TLS)
property.account_sendtype.smtptlsssl=SMTP(SSL&TLS)

property.account_user=所属用户

