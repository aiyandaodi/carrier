property.message_record_receiver=接收者
property.message_record_forward=转发
property.message_record_sender=发送者

property.message_record_time=收发时间
property.message_record_ontime=定时发送

property.message_record_account=使用账号
property.message_record_owner=所有者
property.message_record_content=内容

property.message_record_box=存放信箱
property.message_record_archive=归档标识
property.message_record_attach=附件文件名

property.message_record_status=信息状态
property.message_record_status.failure=发送失败
property.message_record_status.ready=准备发送
property.message_record_status.sent=已经发送
property.message_record_status.recv=收到信息
property.message_record_status.read=已经读取的接收信息

