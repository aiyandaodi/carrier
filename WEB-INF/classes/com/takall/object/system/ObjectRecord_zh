name=服务主机

#property
property.service_provider_id=标识
property.service_provider_code=编码
property.service_provider_name=名称

property.service_provider_url=主机网址
property.service_provider_port=主机端口

property.service_provider_status=主机状态
property.service_provider_protocol=服务协议
property.service_provider_version=服务版本
property.service_provider_default=是否该协议默认服务
property.service_provider_param=其他参数 

property.service_provider_appid=服务APPID


