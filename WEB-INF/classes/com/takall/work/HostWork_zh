name=任务执行

#property
property.host_work_id=标识
property.host_work_code=编码
property.host_work_name=工作名称
property.host_work_type=工作类型
property.host_work_type.auto=自动执行
property.host_work_type.notauto=手动执行
property.host_work_owner=工作所有者
property.host_work_task=任务标识
property.host_work_status=工作进行的状态值
property.host_work_status.stop=执行完毕
property.host_work_status.error=错误
property.host_work_status.valid=正常运行
property.host_work_error=执行的错误数
property.host_work_start=有效开始时间
property.host_work_end=有效结束时间
