name=主机服务

#property
property.host_service_id=标识
property.host_service_code=编码
property.host_service_name=服务名称
property.host_service_site=使用该服务的主机
property.host_service_type=服务类型
property.host_service_value=服务有效值
property.host_service_value.wait=准备提供
property.host_service_value.apply=不再有效
property.host_service_value.valid=正常服务
property.host_service_port=该服务端口
property.host_service_ifmonitor=服务是否可监控
property.host_service_ifmonitor.wait=准备监控
property.host_service_ifmonitor.apply=不可监控
property.host_service_ifmonitor.valid=可监控
property.host_service_ismonitor=服务是否已经监控
property.host_service_ismonitor.wait=准备监控
property.host_service_ismonitor.apply=未监控
property.host_service_ismonitor.valid=已监控
property.host_service_remark=该服务的备注
