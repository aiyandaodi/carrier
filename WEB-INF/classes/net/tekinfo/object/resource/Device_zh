property.device_ini.imap=IMAP协议
property.device_ini.weixin=微信
property.device_ini.sc1000=SC读卡器
property.device_ini.zkh1=中控指纹打卡机
property.device_ini.weixin_qrcode=微信二维码支付
property.device_creator=创建者
property.device_status=状态
property.device_status.stop=停用
property.device_status.apply=待审核
property.device_status.normal=正常

property.device_baudrate=每秒位数
property.device_baudrate.b110=110
property.device_baudrate.b300=300
property.device_baudrate.b1200=1200
property.device_baudrate.b2400=2400
property.device_baudrate.b4800=4800
property.device_baudrate.b9600=9600
property.device_baudrate.b19200=19200
property.device_baudrate.b38400=38400
property.device_baudrate.b57600=57600
property.device_baudrate.b115200=115200
property.device_baudrate.b230400=230400
property.device_baudrate.b460800=460800
property.device_baudrate.b921600=921600
property.device_databits=数据位
property.device_databits.d5=5
property.device_databits.d6=6
property.device_databits.d7=7
property.device_databits.d8=8
property.device_flowcontrol=流控制
property.device_flowcontrol.none=无
property.device_flowcontrol.rtsctsin=RTS/CTS IN
property.device_flowcontrol.rtsctsout=RTS/CTS OUT
property.device_flowcontrol.xonxoffin=XON/XOFF IN
property.device_flowcontrol.xonxoffout=XON/XOFF OUT
property.device_parity=校验方式
property.device_parity.none=无
property.device_parity.odd=奇校验
property.device_parity.even=偶校验
property.device_parity.mark=标记
property.device_parity.space=空格
property.device_stopbits=停止位
property.device_stopbits.s1=1
property.device_stopbits.s1_5=1.5
property.device_stopbits.s2=2
property.device_center=短信中心号码
property.device_sendspeed=发送速率（条/分钟）

#message
message.error.existAccount=存在使用该端口设备的账号
message.error.existAddress=端口已经使用!
message.error.invalidPort=端口号错误!
message.error.invalidBaudrate=每秒位数错误!
message.error.invalidDatabits=数据位错误!
message.error.invalidFlowControl=流控制错误!
message.error.invalidParity=校验方式错误!
message.error.invalidStopbits=停止位错误!
message.error.invalidIp=IP地址错误!
message.error.sendspeed=发送速率必须大于0!
message.error.delete.isvalid=端口设备状态是“有效”，禁止删除!