name=端口设备

#property
property.device_id=标识
property.device_code=编码
property.device_name=名称
property.device_type=端口类型
property.device_type.com=串口
property.device_type.tcp=TCP端口
property.device_address=端口地址
property.device_parameter=端口参数
property.device_ini=设备类型
property.device_ini.at=AT手机
property.device_ini.gprs=彩信手机
property.device_ini.chuanglan=创蓝
property.device_ini.smtp=SMTP协议
