name=目录分类

#property
property.catalog_id=标识
property.catalog_code=编码
property.catalog_name=分类名称
property.catalog_object=所属对象类型名
property.catalog_owner=所有者

property.catalog_config=目录树配置
property.catalog_base=基值
property.catalog_depth=级数
property.catalog_digit=位数
property.catalog_step=步长
property.catalog_tree=编码类型

#message
message.conj.or=或

message.error.paramError=请求参数错误
message.error.paramFault=请求参数不完整
message.error.paramCatalogObject=参数"所属对象名"无效
message.error.paramCatalogOwner=参数"所属用户"无效
message.error.existChildNode=存在子节点
message.error.nameIsNotEmpty=目录名不能为空值
message.error.codeIsNotEmpty=编码不能为空值
message.error.codeIsWrongful=新建编码不合规范
message.error.codeExisted=编码已经存在
message.error.publicCatalogOnly=只允许建立公共目录
message.error.privateCatalogOnly=只允许建立私有目录
