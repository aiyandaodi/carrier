name=颜色分类

#property
property.color_id=标识
property.color_code=编码
property.color_name=颜色名
property.color_object=所属对象类型名
property.color_owner=所有者
property.color_value=颜色值

property.color_enforce=强制性
property.color_private=私有定义

#message
message.conj.or=或

message.error.paramError=请求参数错误
message.error.paramFault=请求参数不完整
message.error.paramColorObject=参数"所属对象名"无效
message.error.paramColorOwner=参数"所属用户"无效
message.error.codeExisted=编码已经存在
message.error.nameExisted=颜色名已经存在
message.error.notDefinedPrivate=不能定义私有颜色
