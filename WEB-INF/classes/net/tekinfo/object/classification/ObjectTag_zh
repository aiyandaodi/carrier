name=标签分类

#property
property.tag_id=标识
property.tag_code=编码
property.tag_name=标签名
property.tag_object=所属对象类型名
property.tag_owner=所有者
property.tag_language=语言

property.tag_count=最大数量
property.tag_enforce=强制性
property.tag_private=私有定义

#message
message.conj.or=或
message.maxTagCount=最大标签数

message.error.paramError=请求参数错误
message.error.paramFault=请求参数不完整
message.error.paramTagObject=参数"所属对象名"无效
message.error.paramTagOwner=参数"所属用户"无效
message.error.codeExisted=编码已经存在
message.error.nameExisted=标签名已经存在
message.error.notDefinedPrivate=不能定义私有标签
