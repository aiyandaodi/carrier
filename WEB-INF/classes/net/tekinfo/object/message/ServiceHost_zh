property.service_host_id=标识
property.service_host_code=编码
property.service_host_name=名称
property.service_host_url=主机网址
property.service_host_status=主机状态
property.service_host_status.apply=申请
property.service_host_status.stop=停止
property.service_host_status.valid=有效

property.service_host_protocol=服务协议
property.service_host_protocol.http=HTTP
property.service_host_protocol.https=HTTPS
property.service_host_protocol.pop3=POP3
property.service_host_protocol.imap=IMAP
property.service_host_protocol.smtp=SMTP
property.service_host_protocol.sip=SIP网络电话
property.service_host_protocol.xmpp=XMPP即时通
property.service_host_protocol.ftp=FTP文件传送
property.service_host_protocol.ipsee=IPSEE认证

property.service_host_version=服务协议版本
property.service_host_default=是否默认协议
property.service_host_param=参数
property.service_host_appid=应用APPID

