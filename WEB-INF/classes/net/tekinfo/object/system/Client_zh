name=客户端

#property
property.client_id=标识
property.client_code=编码
property.client_name=名称
property.client_token=授权码
property.client_tokenSecret=授权码密钥
property.client_type=类别
property.client_type.system=当前服务器
property.client_type.auth=扩展系统
property.client_type.login=第三方登录
property.client_type.operate=第三方操作
property.client_type.bind=第三方授权
property.client_type.syncDown=下级服务器
property.client_type.syncSame=同级服务器
property.client_type.syncUp=上级服务器
property.client_type.syncOther=其他服务器
property.client_status=状态
property.client_status.apply=申请中
property.client_status.normal=正常
property.client_status.expire=过期
property.client_status.forbid=禁止
property.client_domain=域名(IP地址)
property.client_start=有效期（开始时间）
property.client_end=有效期（结束时间）
property.client_userName=联系人
property.client_phone=联系电话
property.client_email=联系邮箱
property.client_postCode=邮编
property.client_address=地址
property.client_remark=备注
property.client_owner=所有者

property.client_category=客户端种类
property.client_category.login=第三方登录
property.client_category.bind=第三方授权
property.client_category.server=服务器

#message
messge.remark.system=系统设置
message.callbackURL=回调地址
message.main=主页

message.error.noRight=客户端无权限!
message.error.unregister=客户端未注册或未审核!
